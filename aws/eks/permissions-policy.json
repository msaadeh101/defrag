{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "secretsmanager:GetSecretValue",
        "Resource": [
          "arn:aws:secretsmanager:us-east-1:123456789012:secret:myapp/database-*",
          "arn:aws:secretsmanager:us-east-1:123456789012:secret:myapp/api-*"
        ]
      }
    ]
  }
  